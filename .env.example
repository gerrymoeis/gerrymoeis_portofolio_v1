# Supabase Configuration
# Dapatkan nilai ini dari dashboard proyek Supabase Anda
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_public_anon_key

# (Opsional) Kunci ini digunakan untuk operasi backend/admin di API routes atau server functions
# JANGAN ekspos kunci ini di sisi klien
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Next.js Environment
NODE_ENV=development

# Contoh variabel lingkungan lain yang mungkin Anda butuhkan
# API_BASE_URL=http://localhost:3001/api
# NEXTAUTH_URL=http://localhost:3000
# NEXTAUTH_SECRET=a_very_strong_random_secret_string_for_nextauth
